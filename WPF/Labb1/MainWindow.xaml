<Window x:Class="Labb1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Labb1"
        mc:Ignorable="d"
        Title="Areakalkylator" Height="300" Width="250">
    <StackPanel Margin="5">
        <Label Margin="5">Ange Bredd</Label>
        <TextBox Margin="5" Name="rutaBredd"></TextBox>
        <Label Margin="5">Ange höjd</Label>
        <TextBox Margin="5" Name="rutaHöjd"></TextBox>
        <Button Margin="5" Click="KlickRäknaUt">Räkna ut</Button>
        <Label Margin="5">Area</Label>
        <TextBox Margin="5" IsReadOnly="True" Name="rutaResultat"></TextBox>
    </StackPanel>
</Window>
