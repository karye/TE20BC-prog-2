<Window x:Class="Labb2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Labb2"
        mc:Ignorable="d"
        Title="Slump" Height="220" Width="250" Background="#EEE">
    <StackPanel Margin="5">
        <Label FontWeight="Bold">Slumpa fram ett tal</Label>
        <Label >Ange maxgräns</Label>
        <TextBox Margin="5" Name="rutaMax" ></TextBox>
        <Button Margin="5" Padding="5" Click="KlickSlumpa">Slumpa!</Button>
        <Label >Resultat</Label>
        <TextBox Margin="5" Name="rutaResultat"></TextBox>
    </StackPanel>
</Window>
